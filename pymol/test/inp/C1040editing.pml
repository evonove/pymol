# -c

set ray_default_renderer=2

feedback enable, repsurface, blather
feedback enable, isomesh, blather
i 
/print "BEGIN-LOG"

#     alter,              

dele all
load dat/pept.pdb
alter (elem n,o),vdw=4.0
show dots
ray

#     alter_state,    
    
alter_state 1,(all),x=x+y
hide
show lines
ray

dele all
load dat/pept.pdb
load dat/pept.pdb
alter_state -1,(all),x=x+y
alter_state 0,(all), x=x+y
alter_state 1,(all), x=x+y
alter_state 2,(all), x=x+y
alter_state 3,(all), x=x+y

#     h_add,              

dele all
load dat/small01.mol
remove hydro
h_add
count_atoms

#     h_fill,             

dele all
load dat/small01.mol

#     attach,             

dele all
load dat/small01.mol
remove id 17
count_atoms
edit id 3
attach X,4,2
count_atoms
edit elem X
remove_picked
unpick
print cmd.get_names()
count_atoms
h_add
count_atoms

#     bond,               

dele all
load dat/small01.mol
unbond id 1,id 2
h_add
count_atoms
remove hydro
bond id 1,id 2
h_add
count_atoms

#     cycle_valence,      

dele all
load dat/small01.mol
edit id 6,id 10
cycle_valence
count_atoms
cycle_valence
count_atoms
remove_picked
bond id 6,id 10,2
h_add
set valence=0.05
ray

#     deprotect

dele all
load dat/pept.pdb
get_extent
protect resi 1-6
edit 2/sg,12/sg
torsion 90
get_extent
undo
get_extent
redo
get_extent
undo
get_extent
deprotect
edit 2/sg,12/sg
torsion 90
get_extent

#     flag,               
dele all
load dat/pept.pdb
flag ignore,(!*/ca,c,n,o,cb)
flag exfoliate,(*/cb)
show surf
ray

#     fuse
dele all

load dat/small01.mol,m1
load dat/small01.mol,m2
fuse m1 and id 31,m2 and id 31
dele m1
zoom
ray
dele all
load dat/small01.mol,m1
load dat/small01.mol,m2
remove id 30
fuse m1 and id 12,m2 and id 12
dele m1
zoom
ray

#     invert,             

dele all
load dat/small01.mol
edit id 2,id 6, id 5
invert 
print "%8.3f"%cmd.dist("id 28","id 17")

#     iterate,            
#     iterate_state,      

dele all
load dat/pept.pdb
iterate (name n),print resi
iterate_state 1,(name o),print "%8.3f %8.3f %8.3f"%(x,y,z)

#     push_undo

dele all
load dat/il2.pdb
stored.sum = 0
iterate_state 1,(il2),stored.sum=stored.sum+x
print "%8.3f"%stored.sum
push_undo (il2)
alter_state 1,(il2),x=0.0
rebuild
stored.sum = 0
iterate_state 1,(il2),stored.sum=stored.sum+x
print "%8.3f"%stored.sum
edit il2`1
undo
stored.sum = 0
iterate_state 1,(il2),stored.sum=stored.sum+x
print "%8.3f"%stored.sum
rebuild
show dots
ray

#     rename

dele all
load dat/small01.mol
iterate (all),print name
rename small01
iterate (all),print name

#     replace,            

dele all
load dat/small01.mol
edit id 3
replace N,3,3

#     rotate,             

dele all
load dat/pept.pdb
get_extent

# rotate via TTT matrix

origin pept,object=pept
rotate x,-45,object=pept
get_extent
reset
get_extent

# rotate by transforming coordinates

rotate x,45,pept
get_extent

#     set_dihedral,       

dele all
load dat/pept.pdb
get_dihedral 1/n,1/ca,1/c,1/o
get_extent
set_dihedral 1/n,1/ca,1/c,1/o,180
get_dihedral 1/n,1/ca,1/c,1/o
get_extent

#     set_geometry,       

dele all
load dat/small01.mol
remove not id 3
zoom
count_atoms
set_geometry (id 3),4,4
h_add
count_atoms
remove hydro
set_geometry (id 3),3,3
h_add
count_atoms
remove hydro
set_geometry (id 3),2,2
h_add
count_atoms
remove hydro
set_geometry (id 3),4,2
h_add
count_atoms

#     sort

dele all
load dat/pept.pdb
alter (all),resi = (999-int(resi)-ID/4)
sort
show sph,(byres id 10)
ray

dele all

